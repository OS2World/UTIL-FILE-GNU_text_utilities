_as_executable_p="test -f"
function_find_executable()
{ if test -z "$ac_executable_extensions_tested" -a -z "$ac_executable_extensions"; then
    # if there is ls.exe then at least ".exe" should be an executable extension
    if ls.exe -a >/dev/null 2>/dev/null; then
      ac_executable_extensions=".exe"
    else
      ac_executable_extensions=
    fi
  fi

  ac_executable_extensions_tested="1"

  # test for program without extension
  # note: I want to use the shell variable "" but m4 doesn't allow it directly
  #       So I use translit(`$A*', `A')
  if $_as_executable_p $*; then return 0; fi
  # if program not found, try the extensions
  if test -n "$ac_executable_extensions"; then
    for i in "$ac_executable_extensions"; do
      if $_as_executable_p $*"$i"; then return 0; fi
    done
  fi
  return 1;
}
as_executable_p=function_find_executable

